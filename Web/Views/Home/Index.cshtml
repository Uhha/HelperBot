@{
    ViewData["Title"] = "Home Page";
}

@using DatabaseInteractions;

    <div class="text-center">
        <h1 class="display-4">Direct from manager</h1>
        <p>BOTKEY: @System.Configuration.ConfigurationManager.AppSettings["BotApiKey"].Substring(0, 20)</p>
        <p>WebHookUrl: @System.Configuration.ConfigurationManager.AppSettings["WebHookUrl"]</p>
        <p>CoinMarketCapAPIKey: @System.Configuration.ConfigurationManager.AppSettings["CoinMarketCapAPIKey"].Substring(0, 20)</p>
        <p>Environment: @System.Configuration.ConfigurationManager.AppSettings["Environment"]</p>

        <h1 class="display-4">From Config</h1>
        <p>BOTKEY: @Config.BotApiKey</p>
        <p>WebHookUrl: @Config.WebHookUrl</p>
        <p>CoinMarketCapAPIKey: @Config.CoinMarketCapAPIKey</p>
        <p>Environment: @Config.Environment</p>




    </div>
